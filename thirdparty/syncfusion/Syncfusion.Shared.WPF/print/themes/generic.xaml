                            ItemTemplate="{StaticResource PrintersListItemTemplate}"
                            Style="{StaticResource Print_ComboBoxStyle}" ItemsSource="{Binding Printers}"
                            SelectedItem="{Binding SelectedPrinter}"/>
                  <TextBlock x:Name="Part_SettingsTitle" Margin="0,3,10,0" HorizontalAlignment="Left" FontSize="16"
                             Foreground="#000000" Text="{resources:SharedLocalizationResource ResourceName=Settings}"/>
                  <ComboBox x:Name="PART_PageRangeSelection" Margin="0,13,0,4" HorizontalAlignment="Left"
                            SelectedValuePath="PageRangeSelection" Style="{StaticResource Print_ComboBoxStyle}"
                            ItemTemplate="{StaticResource PageRangeSelectionTemplate}"
                            SelectedValue="{Binding SelectedPrintPageRange, Mode=TwoWay}"
                            ItemsSource="{Binding PrintPageRangeSelectionOptions}"/>
                  <StackPanel Margin="5,0,0,4" Orientation="Horizontal">
                    <TextBlock x:Name="Part_Pages" Foreground="{StaticResource MetroForegroundBrush}" HorizontalAlignment="Left"
                               VerticalAlignment="Center" FontSize="13"
                               Text="{Binding Source={resources:SharedLocalizationResource ResourceName=PrintPreview_Pages}, StringFormat={0}:}"/>
