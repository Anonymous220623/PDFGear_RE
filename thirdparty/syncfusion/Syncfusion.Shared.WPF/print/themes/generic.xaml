                                        Margin=".5,1,.5,0" Grid.Row="0" Grid.Column="1" Style="{StaticResource RepeatButtonStyle}"
                                        Width="15">
                            <Path x:Name="Copy_upbuttonpath" Stretch="Uniform" Fill="{StaticResource RepeatButton_Fill}" Height="6"
                                  Width="6" Data="M 0,0 L 3.5,4 7,0 Z" HorizontalAlignment="Center" VerticalAlignment="Center"
                                  RenderTransformOrigin=".5,.5" Margin="-1">
                              <UIElement.RenderTransform>
                                <TransformGroup>
                                  <ScaleTransform/>
                                  <SkewTransform/>
                                  <RotateTransform Angle="180"/>
                                  <TranslateTransform/>
                                </TransformGroup>
                              </UIElement.RenderTransform>
                            </Path>
                          </RepeatButton>
                          <RepeatButton x:Name="Copy_downbutton" IsTabStop="false" Margin=".5,0,.5,.5" Grid.Row="2" Grid.Column="1"
                                        Style="{StaticResource RepeatButtonStyle}" Width="15">
                            <Path x:Name="Copy_downbuttonpath" Stretch="Uniform" Fill="{StaticResource RepeatButton_Fill}" Height="6"
                                  Width="6" Data="M 0,0 L 3.5,4 7,0 Z" HorizontalAlignment="Center" VerticalAlignment="Center"
                                  Margin="-1"/>
                          </RepeatButton>
                        </Grid>
                      </Border>
                    </Grid>
                  </StackPanel>
                  <TextBlock x:Name="Part_PrinterTitle" Margin="0,2,10,0" HorizontalAlignment="Left" FontSize="16"
                             Foreground="#000000"
                             Text="{resources:SharedLocalizationResource ResourceName=PrintPreview_Printer}"/>
