                      </Geometry>
                    </Button>
                    <TextBox x:Name="PART_TextBox" Margin="5,0,0,10" Width="20" Height="15" FontFamily="Roboto-Regular"
                             HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Left"
                             VerticalContentAlignment="Center" FontSize="9" Text="{Binding PageIndex}">
                    </TextBox>
                    <TextBlock x:Name="PART_TextBlock" HorizontalAlignment="Center" Margin="2,0,0,10" Width="27"
                               FontFamily="Roboto-Regular" VerticalAlignment="Center" TextAlignment="Center" FontSize="9"
                               Text="{Binding TotalPages, StringFormat='/  {0}'}"/>
                    <Button x:Name="NextPageButton" Margin="2,0,0,10" Foreground="#FF6A6A6A"
                            Style="{StaticResource NavigationBtnStyle}" Command="{Binding NextCommand}">
                      <Geometry>M0,0 L7,4 0,8 z</Geometry>
                    </Button>
                    <Button x:Name="LastPageButton" Margin="5,0,0,10" Foreground="#FF6A6A6A"
                            Style="{StaticResource NavigationBtnStyle}" Command="{Binding LastCommand}">
                      <Geometry>
                        M8,0 L10,0 10,8.0000001 8,8.0000001 z M0,0 L7.0830002,4.0000001 0,8.0000001 z
                      </Geometry>
                    </Button>
                  </StackPanel>
                  <Grid HorizontalAlignment="Right" DockPanel.Dock="Right" Margin="0,0,0,10">
                    <Grid.ColumnDefinitions>
                      <ColumnDefinition Width="Auto"/>
                      <ColumnDefinition Width="Auto"/>
                      <ColumnDefinition Width="Auto"/>
                      <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock x:Name="PART_ZoomPercentage" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10"
                               Text="{Binding ElementName=PART_ZoomSlider, Path=Value, StringFormat={0:#}%}"/>
                    <Button x:Name="PART_PlusZoomButton" Margin="6" Grid.Column="3" Focusable="false" Width="12" Height="14"
                            HorizontalAlignment="Center" Style="{StaticResource NavigationBtnStyle}">
                      <Geometry>
                        M3,0 L5,0 5,3 8,3 8,5 5,5 5,8 3,8 3,5 0,5 0,3 3,3 z
                      </Geometry>
                    </Button>
                    <Slider x:Name="PART_ZoomSlider" Grid.Column="2" Foreground="#CFCFCF" Width="150" Height="20"
                            Focusable="false" HorizontalAlignment="Center" VerticalAlignment="Center"
                            HorizontalContentAlignment="Center" Maximum="500" Minimum="10" Orientation="Horizontal"
                            IsMoveToPointEnabled="true" Value="{Binding ZoomFactor}"/>
                    <Button x:Name="PART_MinusZoomButton" Margin="6" Grid.Column="1" Focusable="false" Width="12" Height="8"
                            HorizontalAlignment="Center" Style="{StaticResource NavigationBtnStyle}">
                      <Geometry>M0,0 L8,0 8,2 0,2 z</Geometry>
                    </Button>
                  </Grid>
                </DockPanel>
              </Grid>
            </Grid>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style BasedOn="{StaticResource PrintPreviewStyle}" TargetType="{x:Type printing:PrintPreview}"/>
</ResourceDictionary>