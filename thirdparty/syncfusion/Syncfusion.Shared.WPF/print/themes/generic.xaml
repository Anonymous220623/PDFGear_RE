                               Text="{Binding Source={resources:SharedLocalizationResource ResourceName=PrintPreview_Copies}, StringFormat={0}:}"/>
                    <Grid x:Name="Part_CopyCount_Grid" Width="60" Height="24" HorizontalAlignment="Left"
                          VerticalAlignment="Top" Margin="5,18,0,0">
