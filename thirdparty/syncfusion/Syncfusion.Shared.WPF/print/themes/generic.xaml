                                   Width="{TemplateBinding FrameworkElement.Width}" Height="{TemplateBinding FrameworkElement.Height}"
                                   BorderThickness="0" BorderBrush="#DEDEDE" Foreground="{StaticResource TextBox_Foreground}"
                                   Background="{StaticResource TextBox_Background}" HorizontalContentAlignment="Left"
                                   VerticalContentAlignment="Center" Text="{Binding FromPage, Mode=TwoWay}"/>
                          <RepeatButton x:Name="FromPage_upbutton" Focusable="{TemplateBinding UIElement.Focusable}" IsTabStop="false"
                                        Margin=".5,1,.5,0" Grid.Row="0" Grid.Column="1" Style="{StaticResource RepeatButtonStyle}"
                                        Width="15">
                            <Path x:Name="FromPage_upbuttonpath" Stretch="Uniform" Fill="{StaticResource RepeatButton_Fill}"
                                  Height="6" Width="6" Data="M 0,0 L 3.5,4 7,0 Z" HorizontalAlignment="Center"
                                  VerticalAlignment="Center" RenderTransformOrigin=".5,.5" Margin="-1">
                              <UIElement.RenderTransform>
                                <TransformGroup>
                                  <ScaleTransform/>
                                  <SkewTransform/>
                                  <RotateTransform Angle="180"/>
                                  <TranslateTransform/>
                                </TransformGroup>
                              </UIElement.RenderTransform>
                            </Path>
                          </RepeatButton>
                          <RepeatButton x:Name="FromPage_downbutton" IsTabStop="false" Margin=".5,0,.5,.5" Grid.Row="2" Grid.Column="1"
                                        Style="{StaticResource RepeatButtonStyle}" Width="15">
                            <Path x:Name="FromPage_downbuttonpath" Stretch="Uniform" Fill="{StaticResource RepeatButton_Fill}"
                                  Height="6" Width="6" Data="M 0,0 L 3.5,4 7,0 Z" HorizontalAlignment="Center"
                                  VerticalAlignment="Center" Margin="-1"/>
                          </RepeatButton>
                        </Grid>
                      </Border>
                    </Grid>
                    <TextBlock x:Name="Part_To" Foreground="{StaticResource MetroForegroundBrush}" HorizontalAlignment="Left"
                               VerticalAlignment="Center" FontSize="13"
                               Text="{resources:SharedLocalizationResource ResourceName=PrintPreview_To}"/>
                    <Grid x:Name="Part_ToPage_Grid" Width="60" Height="24" HorizontalAlignment="Left" VerticalAlignment="Top"
                          Margin="5,0,8,0">
                      <Border x:Name="ToPage_Border" Width="{TemplateBinding FrameworkElement.Width}" SnapsToDevicePixels="true"
                              BorderThickness="1" BorderBrush="{StaticResource RepeatButtonBorderBrush}" Background="White">
                        <Grid ClipToBounds="true">
                          <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                          </Grid.ColumnDefinitions>
                          <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                          </Grid.RowDefinitions>
                          <TextBox x:Name="Part_ToPageTextBox" Grid.Column="0" Grid.RowSpan="2"
                                   Width="{TemplateBinding FrameworkElement.Width}" Height="{TemplateBinding FrameworkElement.Height}"
                                   BorderThickness="0" BorderBrush="#DEDEDE" Foreground="{StaticResource TextBox_Foreground}"
                                   Background="{StaticResource TextBox_Background}" HorizontalContentAlignment="Left"
                                   VerticalContentAlignment="Center" Text="{Binding ToPage, Mode=TwoWay}"/>
                          <RepeatButton x:Name="ToPage_upbutton" Focusable="{TemplateBinding UIElement.Focusable}" IsTabStop="false"
                                        Margin=".5,1,.5,0" Grid.Row="0" Grid.Column="1" Style="{StaticResource RepeatButtonStyle}"
                                        Width="15">
                            <Path x:Name="ToPage_upbuttonpath" Stretch="Uniform" Fill="{StaticResource RepeatButton_Fill}" Height="6"
                                  Width="6" Data="M 0,0 L 3.5,4 7,0 Z" HorizontalAlignment="Center" VerticalAlignment="Center"
                                  RenderTransformOrigin=".5,.5" Margin="-1">
                              <UIElement.RenderTransform>
                                <TransformGroup>
                                  <ScaleTransform/>
                                  <SkewTransform/>
                                  <RotateTransform Angle="180"/>
                                  <TranslateTransform/>
                                </TransformGroup>
                              </UIElement.RenderTransform>
                            </Path>
                          </RepeatButton>
                          <RepeatButton x:Name="ToPage_downbutton" IsTabStop="false" Margin=".5,0,.5,.5" Grid.Row="2" Grid.Column="1"
                                        Style="{StaticResource RepeatButtonStyle}" Width="15">
                            <Path x:Name="ToPage_downbuttonpath" Stretch="Uniform" Fill="{StaticResource RepeatButton_Fill}"
                                  Height="6" Width="6" Data="M 0,0 L 3.5,4 7,0 Z" HorizontalAlignment="Center"
                                  VerticalAlignment="Center" Margin="-1"/>
                          </RepeatButton>
                        </Grid>
                      </Border>
                    </Grid>
                  </StackPanel>
                  <ComboBox x:Name="PART_Collated" Margin="0,0,0,4" HorizontalAlignment="Left" SelectedValuePath="Collation"
                            Style="{StaticResource Print_ComboBoxStyle}" ItemTemplate="{StaticResource CollatedTemplate}"
                            SelectedValue="{Binding SelectedCollation, Mode=TwoWay}"
                            ItemsSource="{Binding PrintPageCollationOptions}"/>
                  <ComboBox x:Name="PART_OrientationComboBox" Margin="0,0,0,4" HorizontalAlignment="Left"
                            SelectedValuePath="Orientation" Style="{StaticResource Print_ComboBoxStyle}"
                            ItemTemplate="{StaticResource OrientationTemplate}"
                            SelectedValue="{Binding PrintOrientation, Mode=TwoWay}" ItemsSource="{Binding OrientationOptions}"/>
                  <Grid>
                    <ComboBox x:Name="PART_PapersComboBox" Margin="0,0,0,4" HorizontalAlignment="Left"
                              ItemTemplate="{StaticResource PageSizeTemplate}" Style="{StaticResource Print_ComboBoxStyle}"
                              ItemsSource="{Binding PageSizeOptions}" SelectedItem="{Binding SelectedPageSize}"/>
                    <Grid Width="Auto" Height="Auto">
                      <TextBlock IsHitTestVisible="false" VerticalAlignment="Center" Margin="10,0,0,10" FontWeight="Normal"
                                 Foreground="{x:Static JetSystemColors.GrayTextBrush}">
                        <FrameworkElement.Style>
                          <Style TargetType="{x:Type TextBlock}">
                            <Setter Property="TextBlock.Text" Value="Choose any PageSize"/>
                            <Setter Property="UIElement.Visibility" Value="Hidden"/>
                            <Style.Triggers>
                              <DataTrigger Binding="{Binding Path=Items.Count, ElementName=PART_PapersComboBox}" Value="0">
                                <Setter Property="TextBlock.Text" Value="Unable to connect"/>
                              </DataTrigger>
                              <DataTrigger Binding="{Binding Path=SelectedIndex, ElementName=PART_PapersComboBox}" Value="-1">
                                <Setter Property="UIElement.Visibility" Value="Visible"/>
                              </DataTrigger>
                            </Style.Triggers>
                          </Style>
                        </FrameworkElement.Style>
                      </TextBlock>
                    </Grid>
                  </Grid>
                  <ComboBox x:Name="PART_MarginComboBox" Margin="0,0,0,4" HorizontalAlignment="Left"
                            ItemTemplate="{StaticResource MarginTemplate}" Style="{StaticResource Print_ComboBoxStyle}"
                            ItemsSource="{Binding MarginOptions}"/>
                  <ComboBox x:Name="PART_ScaleOptionComboBox" Margin="0,0,0,4" HorizontalAlignment="Left"
                            ItemTemplate="{StaticResource ScaleOptionTemplate}" Style="{StaticResource Print_ComboBoxStyle}"
                            SelectedIndex="{Binding SelectedScaleIndex}" ItemsSource="{Binding ScaleOptions}"/>
                </StackPanel>
              </Grid>
            </ScrollViewer>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
