    <Setter Property="Control.Background" Value="White"/>
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type printing:PrintOptionsControl}">
          <Border x:Name="PART_Border" BorderBrush="#E9E9E9" BorderThickness="0,0,1,0">
            <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
              <Grid HorizontalAlignment="Center">
                <StackPanel>
                  <StackPanel Orientation="Horizontal">
                    <Button x:Name="Part_QuickPrintButton" Margin="0,12,17,20" Style="{StaticResource Print_ButtonStyle}"
