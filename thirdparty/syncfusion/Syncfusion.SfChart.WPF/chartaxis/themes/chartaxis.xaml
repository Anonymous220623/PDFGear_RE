                        Fill="White" Stretch="Uniform" Stroke="Black" StrokeThickness="3" Width="31" Height="31"
                        Grid.Row="0" Margin="0,0,0,-2"/>
                  <Path Data="M 299.906,317.548 L 314.401,340.841 329.346,316.701 C 329.346,316.701 316.448,324.029 299.906,317.548 Z"
                        Fill="Black" Height="6.714" Stretch="Fill" StrokeThickness="6" UseLayoutRounding="false"
                        Width="10.437" Grid.Row="1"/>
                  <UIElement.RenderTransform>
                    <RotateTransform Angle="180"/>
                  </UIElement.RenderTransform>
                </Grid>
              </ControlTemplate>
              <ControlTemplate x:Key="HorizontalFarThumbTemplate" TargetType="{x:Type Thumb}">
                <Grid x:Name="Root" Width="35">
                  <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                  </Grid.RowDefinitions>
                  <Path Data="M 23.5,12.5 C 23.5,18.5751 18.5751,23.5 12.5,23.5 6.42487,23.5 1.5,18.5751 1.5,12.5 1.5,6.42487 6.42487,1.5 12.5,1.5 18.5751,1.5 23.5,6.42487 23.5,12.5 Z"
                        Fill="White" Stretch="Uniform" Stroke="Black" StrokeThickness="3" Width="31" Height="31"
                        Grid.Row="0" Margin="0,0,0,-2"/>
                  <Path Data="M 299.906,317.548 L 314.401,340.841 329.346,316.701 C 329.346,316.701 316.448,324.029 299.906,317.548 Z"
                        Fill="Black" Height="6.714" Stretch="Fill" StrokeThickness="6" UseLayoutRounding="false"
                        Width="10.437" Grid.Row="1"/>
                  <UIElement.RenderTransform>
                    <RotateTransform Angle="180"/>
                  </UIElement.RenderTransform>
                </Grid>
              </ControlTemplate>
              <ControlTemplate x:Key="VerticalNearThumbTemplate" TargetType="{x:Type Thumb}">
                <Grid x:Name="Root" Width="62">
                  <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                  </Grid.RowDefinitions>
                  <Path Data="M 23.5,12.5 C 23.5,18.5751 18.5751,23.5 12.5,23.5 6.42487,23.5 1.5,18.5751 1.5,12.5 1.5,6.42487 6.42487,1.5 12.5,1.5 18.5751,1.5 23.5,6.42487 23.5,12.5 Z"
                        Fill="White" Stretch="Uniform" Stroke="Black" StrokeThickness="3" Grid.Row="0"/>
                  <Path Data="M 299.906,317.548 L 314.401,340.841 329.346,316.701 C 329.346,316.701 316.448,324.029 299.906,317.548 Z"
                        Fill="Black" Height="6.714" Stretch="Fill" StrokeThickness="6" UseLayoutRounding="false"
                        Width="10.437" Grid.Row="1" Margin="0,-28,0,0"/>
                  <UIElement.RenderTransform>
                    <RotateTransform Angle="270"/>
                  </UIElement.RenderTransform>
                </Grid>
              </ControlTemplate>
              <ControlTemplate x:Key="VerticalFarThumbTemplate" TargetType="{x:Type Thumb}">
                <Grid x:Name="Root" Width="62">
                  <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                  </Grid.RowDefinitions>
                  <Path Data="M 23.5,12.5 C 23.5,18.5751 18.5751,23.5 12.5,23.5 6.42487,23.5 1.5,18.5751 1.5,12.5 1.5,6.42487 6.42487,1.5 12.5,1.5 18.5751,1.5 23.5,6.42487 23.5,12.5 Z"
