  <Style TargetType="{x:Type local:ChartAxisBase2D}">
    <Setter Property="local:ChartAxis.MajorTickLineStyle" Value="{StaticResource lineStyle}"/>
    <Setter Property="local:ChartAxis.MinorTickLineStyle" Value="{StaticResource lineStyle}"/>
    <Setter Property="local:ChartAxis.MajorGridLineStyle" Value="{StaticResource gridLineStyle}"/>
    <Setter Property="local:ChartAxis.MinorGridLineStyle" Value="{StaticResource gridLineStyle}"/>
    <Setter Property="local:ChartAxis.OriginLineStyle" Value="{StaticResource gridLineStyle}"/>
    <Setter Property="local:ChartAxis.AxisLineStyle" Value="{StaticResource lineStyle}"/>
    <Setter Property="Control.FontSize" Value="10"/>
    <Setter Property="local:ChartAxisBase2D.LabelBorderBrush" Value="Black"/>
    <Setter Property="Control.Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:ChartAxisBase2D}">
          <Grid>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="StyleMode">
                <VisualState x:Name="CommonStyle">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="sfchartResizableBar" Storyboard.TargetProperty="(FrameworkElement.Style)">
                      <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource sfResizableBarDesktopStyle}"/>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="sfchartResizableBar" Storyboard.TargetProperty="ScrollButtonVisibility">
                      <DiscreteObjectKeyFrame KeyTime="00:00:00">
                        <ObjectKeyFrame.Value>
                          <Visibility>Visible</Visibility>
                        </ObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="TouchModeStyle">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="sfchartResizableBar" Storyboard.TargetProperty="(FrameworkElement.Style)">
                      <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource sfResizableBarStyle}"/>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="sfchartResizableBar" Storyboard.TargetProperty="ScrollButtonVisibility">
                      <DiscreteObjectKeyFrame KeyTime="00:00:00">
                        <ObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </ObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="OpposedTouchModeStyle">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="sfchartResizableBar" Storyboard.TargetProperty="(FrameworkElement.Style)">
                      <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource sfResizableBarStyleOpposed}"/>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Storyboard.TargetName="sfchartResizableBar" Storyboard.TargetProperty="ScrollButtonVisibility">
                      <DiscreteObjectKeyFrame KeyTime="00:00:00">
                        <ObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </ObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Background="{TemplateBinding Control.Background}"
                    BorderThickness="{TemplateBinding Control.BorderThickness}"
                    BorderBrush="{TemplateBinding Control.BorderBrush}" VerticalAlignment="Stretch"
                    HorizontalAlignment="Stretch">
              <local:ChartCartesianAxisPanel x:Name="axisPanel" Background="Transparent">
                <ContentControl x:Name="headerContent" Content="{TemplateBinding local:ChartAxis.Header}" FontSize="12"
                                ContentTemplate="{TemplateBinding local:ChartAxis.HeaderTemplate}" RenderTransformOrigin="0.5,0.5"/>
                <Canvas x:Name="axisMultiLevelLabelsPanels"/>
                <Canvas x:Name="axisLabelsPanel"/>
                <Canvas x:Name="axisElementPanel"/>
                <local:SfChartResizableBar x:Name="sfchartResizableBar"
                                           EnableTouchMode="{TemplateBinding local:ChartAxisBase2D.EnableTouchMode}"/>
              </local:ChartCartesianAxisPanel>
            </Border>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="3Dgridlinestyle" TargetType="{x:Type Line}">
    <Setter Property="Shape.Stroke" Value="#F1F1F1"/>
    <Setter Property="Shape.StrokeThickness" Value="1"/>
  </Style>
  <Style TargetType="{x:Type local:ChartAxis}">
    <Setter Property="local:ChartAxis.MajorTickLineStyle" Value="{StaticResource lineStyle}"/>
    <Setter Property="local:ChartAxis.MinorTickLineStyle" Value="{StaticResource lineStyle}"/>
    <Setter Property="local:ChartAxis.MajorGridLineStyle" Value="{StaticResource 3Dgridlinestyle}"/>
    <Setter Property="local:ChartAxis.MinorGridLineStyle" Value="{StaticResource 3Dgridlinestyle}"/>
    <Setter Property="local:ChartAxis.AxisLineStyle" Value="{StaticResource lineStyle}"/>
  </Style>
</ResourceDictionary>