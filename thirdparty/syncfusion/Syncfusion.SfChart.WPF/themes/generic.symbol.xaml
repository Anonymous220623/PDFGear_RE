          Data="F1 M 133.308,46.7057 L 144.803,66.6124 156.295,86.5191 133.308,86.5191 110.324,86.5191 121.812,66.6124 133.308,46.7057 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationTriangle">
    <Path Stretch="Fill" Stroke="{TemplateBinding Control.Background}"
          Data="F1 M 133.308,46.7057 L 144.803,66.6124 156.295,86.5191 133.308,86.5191 110.324,86.5191 121.812,66.6124 133.308,46.7057 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="Plus">
    <Path Stretch="Fill" Stroke="{TemplateBinding Control.BorderBrush}"
          Fill="{TemplateBinding Control.Background}"
          Data="F1 M 145.193,54.8249 L 169.315,54.8249 169.315,78.9463 145.193,78.9463 145.193,103.074 121.071,103.074 121.071,78.9463 96.946,78.9463 96.946,54.8249 121.071,54.8249 121.071,30.6983 145.193,30.6983 145.193,54.8249 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationPlus">
    <Path Stretch="Fill" Stroke="{TemplateBinding Control.Background}"
          Data="F1 M 145.193,54.8249 L 169.315,54.8249 169.315,78.9463 145.193,78.9463 145.193,103.074 121.071,103.074 121.071,78.9463 96.946,78.9463 96.946,54.8249 121.071,54.8249 121.071,30.6983 145.193,30.6983 145.193,54.8249 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="Circle">
    <Ellipse Fill="{TemplateBinding Control.Background}" Stroke="{TemplateBinding Control.BorderBrush}"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationCircle">
    <Ellipse Fill="{TemplateBinding Control.Background}" Stroke="Black"/>
  </ControlTemplate>
  <ControlTemplate x:Key="Ellipse">
    <Ellipse Fill="{TemplateBinding Control.Background}" Stroke="{TemplateBinding Control.BorderBrush}"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationEllipse">
    <Ellipse Stroke="{TemplateBinding Control.Background}"/>
  </ControlTemplate>
  <ControlTemplate x:Key="Square">
    <Rectangle Fill="{TemplateBinding Control.Background}" Stroke="{TemplateBinding Control.BorderBrush}"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationSquare">
    <Rectangle Stroke="{TemplateBinding Control.Background}"/>
  </ControlTemplate>
  <ControlTemplate x:Key="Pentagon">
    <Path Fill="{TemplateBinding Control.Background}" Stroke="{TemplateBinding Control.BorderBrush}"
          Stretch="Fill"
          Data="F1 M 147.833,76.8112 L 145.036,68.2032 152.36,62.8845 159.683,68.2032 156.885,76.8112 147.833,76.8112 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationPentagon">
    <Path Stroke="{TemplateBinding Control.Background}" Stretch="Fill"
          Data="F1 M 147.833,76.8112 L 145.036,68.2032 152.36,62.8845 159.683,68.2032 156.885,76.8112 147.833,76.8112 Z"/>
  </ControlTemplate>
