  </ControlTemplate>
  <ControlTemplate x:Key="Hexagon">
    <Path Stretch="Fill" Stroke="{TemplateBinding Control.BorderBrush}"
          Fill="{TemplateBinding Control.Background}"
          Data="F1 M 111.119,104.776 L 88.7819,66.0827 111.119,27.3893 155.795,27.3893 178.133,66.0827 155.795,104.776 111.119,104.776 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationHexagon">
    <Path Stretch="Fill" Stroke="{TemplateBinding Control.Background}"
          Data="F1 M 111.119,104.776 L 88.7819,66.0827 111.119,27.3893 155.795,27.3893 178.133,66.0827 155.795,104.776 111.119,104.776 Z"/>
  </ControlTemplate>
  <ControlTemplate x:Key="HorizontalLine">
    <Line X1="0" X2="{TemplateBinding FrameworkElement.Width}" Y1="0" Y2="0"
          Stroke="{TemplateBinding Control.Background}" StrokeThickness="2" VerticalAlignment="Center"
          HorizontalAlignment="Center"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationHorizontalLine">
    <Line X1="0" X2="{TemplateBinding FrameworkElement.Width}" Y1="0" Y2="0"
          Stroke="{TemplateBinding Control.Background}" StrokeThickness="2" VerticalAlignment="Center"
          HorizontalAlignment="Center"/>
  </ControlTemplate>
  <ControlTemplate x:Key="VerticalLine">
    <Line X1="0" X2="0" Y1="0" Y2="{TemplateBinding FrameworkElement.Height}"
          Stroke="{TemplateBinding Control.Background}" StrokeThickness="2" VerticalAlignment="Center"
          HorizontalAlignment="Center"/>
  </ControlTemplate>
  <ControlTemplate x:Key="AnimationVerticalLine">
    <Line X1="0" X2="0" Y1="0" Y2="{TemplateBinding FrameworkElement.Height}"
          Stroke="{TemplateBinding Control.Background}" StrokeThickness="2" VerticalAlignment="Center"
          HorizontalAlignment="Center"/>
  </ControlTemplate>
  <ControlTemplate x:Key="InvertedTriangle">
    <Path Stretch="Fill" Stroke="{TemplateBinding Control.BorderBrush}"
          Fill="{TemplateBinding Control.Background}"
