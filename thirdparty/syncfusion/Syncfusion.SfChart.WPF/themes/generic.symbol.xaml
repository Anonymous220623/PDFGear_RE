          Fill="{TemplateBinding Control.Background}"
          Data="F1 M 133.133,45.7109 L 154.307,24.5363 175.482,45.7109 154.307,66.8856 175.482,88.0603 154.307,109.235 133.133,88.0603 111.958,109.235 90.7835,88.0603 111.958,66.8856 90.7835,45.7109 111.958,24.5363 133.133,45.7109 Z"/>
