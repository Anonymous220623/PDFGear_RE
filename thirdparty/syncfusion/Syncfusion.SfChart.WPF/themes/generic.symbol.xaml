<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Syncfusion.UI.Xaml.Charts">
  <DataTemplate x:Key="ZoomIn">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Tag="ZoomIn" Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/ZoomIn.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="ZoomOut">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/ZoomOut.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="ZoomReset">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/Reset.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="ZoomPan">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/ZoomPan.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="Selection">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/Selection.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="DisableZoomIn">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/DisableZoomIn.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="DisableZoomOut">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/DisableZoomOut.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="DisableReset">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/DisableReset.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="DisablePan">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/DisablePan.png"/>
    </Border>
  </DataTemplate>
  <DataTemplate x:Key="DisableSelection">
    <Border Height="30" Width="40" Margin="0,3,3,3">
      <Image Source="pack://application:,,,/Syncfusion.SfChart.WPF;component//Resources/DisableSelection.png"/>
    </Border>
  </DataTemplate>
  <ControlTemplate x:Key="Cross">
