  <DataTemplate x:Key="defaultFinancialTrackBallLabel">
    <Grid>
      <Polygon StrokeThickness="0.3" Stroke="{Binding BorderBrush}" Fill="{Binding Interior}">
        <Polygon.Points>
          <Binding>            <!-- constructor argument:
                 PolygonPoints
            -->

          </Binding>
        </Polygon.Points>
      </Polygon>
      <StackPanel Margin="4" Orientation="Vertical">
        <Grid>
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
          </Grid.ColumnDefinitions>
          <TextBlock FontSize="10" Text="{Binding Source={local:ChartLocalizationResource ResourceName=High}}"
                     Foreground="{Binding Foreground}">
            <Run Text=" :" FontSize="10"/>
          </TextBlock>
          <TextBlock Padding="3,0,0,0" FontSize="10" Grid.Column="1" Foreground="{Binding Foreground}"
                     Text="{Binding High}"/>
        </Grid>
        <Grid>
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
          </Grid.ColumnDefinitions>
          <TextBlock FontSize="10" Foreground="{Binding Foreground}"
                     Text="{Binding Source={local:ChartLocalizationResource ResourceName=Low}}">
            <Run Text=" :" FontSize="10"/>
          </TextBlock>
          <TextBlock Padding="3,0,0,0" FontSize="10" Grid.Column="1" Foreground="{Binding Foreground}"
                     Text="{Binding Low}"/>
        </Grid>
        <Grid>
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
          </Grid.ColumnDefinitions>
          <TextBlock FontSize="10" Foreground="{Binding Foreground}"
                     Text="{Binding Source={local:ChartLocalizationResource ResourceName=Open}}">
            <Run Text=" :" FontSize="10"/>
          </TextBlock>
          <TextBlock Padding="3,0,0,0" FontSize="10" Grid.Column="1" Foreground="{Binding Foreground}"
                     Text="{Binding Open}"/>
        </Grid>
        <Grid>
          <Grid.ColumnDefinitions>
