  <DataTemplate x:Key="SeriesDragInfoVertical">
    <Grid>
      <StackPanel Orientation="Horizontal" Height="{Binding OffsetY}">
        <Grid RenderTransformOrigin="0.5,0.5">
          <UIElement.RenderTransform>
            <RotateTransform Angle="{Binding IsNegative, Converter={StaticResource dragElementRotateConverter}}"/>
          </UIElement.RenderTransform>
          <Path VerticalAlignment="Top" Width="15" Height="20"
                Data="F1 M 753.644,-13.0589 L 753.736,-12.9639 753.557,-12.7816 732.137,8.63641 732.137,29.7119 756.445,5.40851 764.094,-2.24384 764.275,-2.42352 771.834,5.1286 796.137,29.4372 796.137,8.36163 774.722,-13.0589 764.181,-23.5967 753.644,-13.0589 Z"
                Stretch="Uniform" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5" Fill="{Binding Brush}">
            <UIElement.RenderTransform>
              <TransformGroup>
                <TransformGroup.Children>
                  <RotateTransform Angle="180"/>
                  <ScaleTransform ScaleX="1" ScaleY="-1"/>
                </TransformGroup.Children>
              </TransformGroup>
            </UIElement.RenderTransform>
          </Path>
          <Rectangle VerticalAlignment="Stretch" Width="3" Fill="{Binding Brush}"/>
        </Grid>
        <TextBlock VerticalAlignment="Center" FontSize="17" HorizontalAlignment="Center" Foreground="{Binding Brush}"
                   Text="{Binding Delta, StringFormat=.###}"/>
      </StackPanel>
    </Grid>
  </DataTemplate>
