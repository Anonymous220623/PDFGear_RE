<Window x:Class="pdfeditor.Views.PurchasedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pdfeditor.Views"
        xmlns:localprop="clr-namespace:pdfeditor.Properties"
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Title="{StaticResource ApplicationTitle}" Height="432" Width="480">
  <Grid Background="#F5F5F5">
    <TextBlock Height="27" Margin="0,20,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="20"
               Text="{x:Static localprop:Resources.WinPurchasedAlreadySubTitle}"/>
    <Image x:Name="uImg" Source="/pdfeditor;component/Style/Resources/iap/paiduser.png" Height="206"
           Width="310" Margin="0,58,0,0" VerticalAlignment="Top"/>
    <TextBlock FontSize="15" Margin="80,273,80,0" TextWrapping="Wrap" HorizontalAlignment="Center"
               VerticalAlignment="Top" Text="{x:Static localprop:Resources.WinPurchasedTitle}"/>
    <Button Margin="80,336,80,0" VerticalAlignment="Top" Width="88" Height="32" FontSize="15"
            Background="#297AFB" Foreground="White" IsDefault="true"
            Content="{x:Static localprop:Resources.WinBtnOkContent}"/>
  </Grid>
</Window>